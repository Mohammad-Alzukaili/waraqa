{
    "name": "mawdoo3/waraqa",
    "description": "used to handle all articles that pulled from waraqa to mediawiki websites",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "mohammad alzukaili",
            "email": "mohammad.alzukaili@mawdoo3.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Mawdoo3\\Waraqa\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "gumlet/php-image-resize": "1.9.*",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.5",
        "mawdoo3com/waraqa-integration": "^2.0"
    },
    "config": {
        "preferred-install": "dist",
        "platform-check": false
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Mawdoo3\\Waraqa\\WaraqaIntegrationServiceProvider"
            ]
        }
    }
}
